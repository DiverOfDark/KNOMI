{
    "name": "ui-ng",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "cross-env NODE_ENV=production vite build && node compress.cjs",
        "build-silent": "cross-env NODE_ENV=production vite build -l silent && node compress.cjs",
