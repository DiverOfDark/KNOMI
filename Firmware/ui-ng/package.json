        "@sveltejs/vite-plugin-svelte": "^2.4.6",
        "@tsconfig/svelte": "^5.0.2",
        "@types/spark-md5": "^3.0.2",
        "@typescript-eslint/parser": "^6.7.0",
        "brotli": "^1.3.2",
        "cross-env": "^7.0.3",
        "eslint": "^8.49.0",
        "eslint-plugin-svelte": "^2.33.2",
        "prettier": "^3.0.3",
        "prettier-plugin-svelte": "^3.0.3",
        "sass": "^1.68.0",
        "sass-loader": "^8.0.2",
        "svelte": "^4.2.0",
        "svelte-check": "^3.5.1",
